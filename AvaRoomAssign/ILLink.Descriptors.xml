<?xml version="1.0" encoding="utf-8" ?>
<linker>
  <!-- 最小化保留 - 仅保留JSON序列化和数据绑定必需的类型 -->
  <assembly fullname="AvaRoomAssign">
    <!-- JSON序列化上下文 -->
    <type fullname="AvaRoomAssign.Models.ConfigJsonContext" preserve="all" />
    <!-- 数据转换器 -->
    <type fullname="AvaRoomAssign.Models.BoolToInverseBoolConverter" preserve="methods" />
    <type fullname="AvaRoomAssign.Models.IntToBoolConverter" preserve="methods" />
    <type fullname="AvaRoomAssign.Models.MultiBoolConverter" preserve="methods" />
    <type fullname="AvaRoomAssign.Models.BoolToTextConverter" preserve="methods" />
  </assembly>
</linker> 